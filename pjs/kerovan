#ACTION {%1 -> GUERRA}
{
    #variable nicks $nicks,%0
}

#ACTION {%1 -> HOSTILIDAD}
{
    #variable nicks $nicks,%0
}

#ACTION {Botín: %1}
{
    #variable botin %1
}

#ACTION {La puerta %1 está cerrada}
{
    abrir %1;
    sigilar %1
}

#ACTION {Vislumbras una oportunidad de ataque contra %0}
{
    #variable opo %0
}

#ACTION {^El portero dice desde el otro lado: está bien, puedes pasar. Bienvenido al poblado}
{
    de
}

#ACTION {^Finalmente la puerta comienza a abrirse dejandote el paso libre hacia en interior de la ciudad}
{
    de;
    cr fu
}

#ACTION {^Nombre corto escondite cambiado de %1 a %2}
{
    #variable escondite %2
}

#ACTION {^Nombre corto escondite cmambiado de %1 a %2}
{
    #variable escondite %2
}

#ACTION {^Te faltan %1 Puntos de experiencia para subir de nivel}
{
    #variable xpleft %1
}

#ACTION {^Vislumbras una oportunidad de ataque contra %1}
{
    #variable opo %1;
    #showme (OPO) -> %1
}


